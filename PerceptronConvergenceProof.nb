Cell[BoxData[
 RowBox[{
  RowBox[{"generateSeparableData", "[", 
   RowBox[{"nSamples_", ",", "dim_", ",", "margin_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"wStar", ",", "bStar", ",", "x", ",", "m", ",", "label", ",", 
      RowBox[{"X", "=", 
       RowBox[{"{", "}"}]}], ",", 
      RowBox[{"y", "=", 
       RowBox[{"{", "}"}]}]}], "}"}], ",", 
    RowBox[{"(*", 
     RowBox[{"Random", " ", "but", " ", "fixed", " ", "separator"}], "*)"}], 
    RowBox[{
     RowBox[{"wStar", "=", 
      RowBox[{"Normalize", "[", 
       RowBox[{"RandomReal", "[", 
        RowBox[{
         RowBox[{"NormalDistribution", "[", 
          RowBox[{"0", ",", "1"}], "]"}], ",", "dim"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"bStar", "=", 
      RowBox[{"RandomReal", "[", 
       RowBox[{"NormalDistribution", "[", 
        RowBox[{"0", ",", "0.1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "X", "]"}], "<", "nSamples"}], ",", 
       RowBox[{
        RowBox[{"x", "=", 
         RowBox[{"RandomVariate", "[", 
          RowBox[{
           RowBox[{"NormalDistribution", "[", 
            RowBox[{"0", ",", "1"}], "]"}], ",", "dim"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"m", "=", 
         RowBox[{
          RowBox[{"wStar", ".", "x"}], "+", "bStar"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Abs", "[", "m", "]"}], "<", "margin"}], ",", 
          RowBox[{"Continue", "[", "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"label", "=", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"m", ">", "0"}], ",", "1", ",", 
           RowBox[{"-", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"AppendTo", "[", 
         RowBox[{"X", ",", "x"}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"AppendTo", "[", 
         RowBox[{"y", ",", "label"}], "]"}], ";"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"<|", 
      RowBox[{
       RowBox[{"\"\<X\>\"", "->", "X"}], ",", 
       RowBox[{"\"\<y\>\"", "->", "y"}], ",", 
       RowBox[{"\"\<wStar\>\"", "->", "wStar"}], ",", 
       RowBox[{"\"\<bStar\>\"", "->", "bStar"}]}], "|>"}]}]}], 
   "]"}]}]], "Input",
